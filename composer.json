{
    "name": "1tomany/rich-bundle",
    "description": "Symfony bundle that provides easy scaffolding to build RICH applications",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "psr/log": "^3.0",
        "symfony/dependency-injection": "^7.2",
        "symfony/event-dispatcher": "^7.2",
        "symfony/form": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/security-core": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/validator": "^7.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.90",
        "phpdocumentor/reflection-docblock": "^6.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "suggest": {
        "1tomany/data-uri": "Provides a data URI parser and file object for handling file uploads",
        "symfony/security-bundle": "Provides autowired TokenStorageInterface to extract authenticated users"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\RichBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\RichBundle\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/rich"
    ],
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
