{
    "name": "1tomany/rich-bundle",
    "description": "Symfony bundle that provides easy scaffolding to support RICH applications",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3",
        "symfony/dependency-injection": "^7.1",
        "symfony/event-dispatcher": "^7.1",
        "symfony/http-foundation": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/security-core": "^7.1",
        "symfony/serializer": "^7.1",
        "symfony/validator": "^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\RichBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\RichBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
